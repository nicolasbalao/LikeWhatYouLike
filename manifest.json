{
  "name": "Like what you like",
  "version": "1.0.0",
  "description": "Chrome extension who like youtube video",
  "manifest_version": 3,
  "author": "Me",
  "action": {
    "default_popup": "index.html",
    "default_title": "Like what you like",
    "default_icon": "./assets/icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://youtube.com/"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs"]
}
